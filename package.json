{
  "name": "yc-serverless-live-debug",
  "description": "Live debug of Yandex cloud functions",
  "version": "1.0.0",
  "scripts": {
    "init": "git config core.hooksPath ./scripts/.hooks",
    "lint": "eslint src test --ext .ts",
    "test": "LOG_LEVEL=error mocha -r ts-node/register test/*.spec.ts -t 10000",
    "test:debug": "LOG_LEVEL=debug ts-node test/e2e.ts",
    "example": "ts-node example/debug",
    "build": "./scripts/build.sh",
    "deploy": "./scripts/deploy.sh"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "console-log-level": "^1.4.1",
    "node-fetch": "^2.6.7",
    "pino-pretty": "^9.1.1",
    "ws": "^8.11.0",
    "ydb-sdk": "^3.7.0"
  },
  "devDependencies": {
    "@types/console-log-level": "^1.4.2",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/pino-pretty": "^5.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@yandex-cloud/function-types": "^2.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "author": "Vitaliy Potapov",
  "license": "MIT"
}
