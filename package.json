{
  "name": "yc-serverless-live-debug",
  "description": "Live debug of Yandex cloud functions",
  "version": "1.0.0",
  "scripts": {
    "init": "git config core.hooksPath ./scripts/.hooks",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "LOG_LEVEL=${LOG_LEVEL:-error} mocha -r ts-node/register test/*.spec.ts -t 10000",
    "test:debug": "LOG_LEVEL=debug npm t",
    "example": "ts-node example/debug",
    "toc": "markdown-toc README.md -i",
    "build": "./scripts/build.sh",
    "deploy": "./scripts/deploy.sh"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs"
  },
  "dependencies": {
    "@yandex-cloud/nodejs-sdk": "^2.4.1",
    "cdktf": "^0.15.5",
    "console-log-level": "^1.4.1",
    "constructs": "^10.1.258",
    "node-fetch": "^2.6.7",
    "pino-pretty": "^9.3.0",
    "ws": "^8.12.1",
    "ydb-sdk": "^3.9.0"
  },
  "devDependencies": {
    "@types/console-log-level": "^1.4.2",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/pino-pretty": "^5.0.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@yandex-cloud/function-types": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "globals": "^13.20.0",
    "lint-staged": "^13.1.2",
    "markdown-toc": "^1.2.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "author": "Vitaliy Potapov",
  "license": "MIT"
}
